  m_Script: {fileID: 11500000, guid: d20f0df1dbbcccb48abf69dbcc5cc4f7, type: 3}
  m_Name: MawronExposureDialogue
  m_EditorClassIdentifier: 
  dialogueTitle: Intro
  dialogueEntries:
  - speakerName: 
    text: "Logrif paira diante de um patrulheiro ferido. O \xFAnico sobrevivente.
      Mas este patrulheiro n\xE3o foi abatido pelo arquidem\xF4nio. Seus ferimentos
      s\xE3o mais potentes. Viscerais. Mortais. Impec\xE1veis. Logrif, barbeiro como
      \xE9, jamais... "
    customDelay: 0
    customSound: {fileID: 0}
  - speakerName: Logrif
    text: "Calado, narrador! Ou\xE7a-me, tolo mortal. E tua morte ser\xE1 limpa.
      Quem faz guerra com teu povo? Quem \xE9 esse farsante que se diz um Senhor
      do Escuro?!"
    customDelay: 0
    customSound: {fileID: 0}
  - speakerName: Patrulheiro
    text: Mawron...
    customDelay: 0
    customSound: {fileID: 0}
  - speakerName: 
    text: "Ao mencionar o nome, o jovem agoniza. Sangue escorre de sua boca, e sua
      vida se esvai. A morte limpa, prometida por Logrif, \xE9 mais uma das promessas
      que o arquidem\xF4nio falhou em cumprir."
    customDelay: 0
    customSound: {fileID: 0}
  - speakerName: Logrif
    text: "Curioso. Um nome amaldi\xE7oado?! Por que n\xE3o pensei nisso antes! Maldito
      novato estiloso! Mostrarei a ele quem \xE9 que manda!"
    customDelay: 0
    customSound: {fileID: 0}
  backgroundMusic: {fileID: 8300000, guid: 093617ec03826524bb8b5f6e136c5354, type: 3}
  stopCurrentMusicWhenPlaying: 1
